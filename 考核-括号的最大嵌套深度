int maxDepth(char * s){
    //if(s[0]=='('&&s[1]==')') return 1;
    int len=strlen(s);
    int st=0;
    int i=0;
    int max=0;
    while(i<len||st!=0){
        if(s[i]=='('){
            st++;
            i++;
            if(max<st) max=st;
            continue;
        }
        else if(s[i]==')'){
            st--;
            i++;
            if(max<st) max=st;
            continue;
        }
        if(max<st) max=st;   
        i++;
    }
    return max;
}
