#include <stdio.h>
#include <string.h>
char s[10005];
char top[10005];

int main() {
	scanf("%s", s);
	int len = strlen(s);
	if (len % 2 != 0)
	{
		printf("false");
		return 0;
	}
	int i, j = 0;
	for (i = 0; i < len; i++) {
		if (s[i] == '(' || s[i] == '[' || s[i] == '{') {
			top[j] = s[i]; //将左括号放入栈里
			++j;
		} else {
			j--;
		}
	}
	if (j == 0)
		printf("true");
	else
		printf("true");
	return 0;
}
