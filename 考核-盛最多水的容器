int maxArea(int* height, int heightSize){
    int i,j,h;
    int max=0,t;
    for(i=0,j=heightSize-1;i<j;){
        if(height[i]<=height[j]){
            h=height[i];
            t=(j-i)*h;
            if(t>max) max=t;
            i++;
            continue;
        }
        else{
            h=height[j];
            t=(j-i)*h;
            if(t>max) max=t;
            j--;
            continue;
        }
    }
    return max;
}
